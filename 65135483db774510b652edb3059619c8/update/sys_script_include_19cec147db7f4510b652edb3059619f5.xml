        <api_name>x_snc_agile.GetBacklogItemStates</api_name>
        <caller_access/>
        <client_callable>false</client_callable>
        <description>Returns an object that maps state ids to their numeric values.</description>
        <mobile_callable>false</mobile_callable>
        <name>GetBacklogItemStates</name>
        <sandbox_callable>false</sandbox_callable>
        <script><![CDATA[function GetBacklogItemStates()
{
	var states = {};
	states.draft     				= 1000;
	states.ready   				 	= 2000;
	states.dev 					    = 3000;
	states.done 				    = 4000;
	states.baselined				= 4500;
	states.test      				= 5000;
	states.failed    				= 6000;
	states.done_showstopper			= 6100;
	states.baselined_showstopper	= 6500;
	states.passed    				= 7000;
	states.prod     				= 8000;
	states.complete  				= 9000;
	states.cancelled 				= 10000;
	return states;
}]]></script>
